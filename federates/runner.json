{
  "name": "helics_co_simulation",
  "federates": [
    {
      "directory": "..",
      "exec": "helics_broker -f 3 --loglevel=debug --name=mainbroker",
      "name": "mainbroker"
    },
    {
      "directory": ".",
      "exec": "python -u FederateGenerator.py federates/Battery.yaml --loglevel=debug",
      "host": "localhost",
      "name": "Battery",
      "wait_for": ["mainbroker"]
    },
    {
      "directory": ".",
      "exec": "python -u FederateGenerator.py federates/Envelope.yaml --loglevel=debug",
      "host": "localhost",
      "name": "Envelope",
      "wait_for": ["mainbroker"]
    },
    {
      "directory": ".",
      "exec": "python -u FederateGenerator.py federates/HeatPump.yaml --loglevel=debug",
      "host": "localhost",
      "name": "HeatPump",
      "wait_for": ["mainbroker"]
    }
  ]
}